generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model data_sht {
  id         Int    @id @default(autoincrement())
  name       String @db.VarChar(255)
  suhu       Float
  kelembapan Float
}

model data_soil {
  id    Int    @id @default(autoincrement())
  name  String @db.VarChar(255)
  value Float
}

model all_sensor {
  id       Int   @id @default(autoincrement())
  soil_hum Float
  dht_hum  Float
  dht_temp Float
}
